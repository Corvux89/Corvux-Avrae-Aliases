<drac2>
a = argparse(&ARGS&)
c = character()
com = combat()
targ = com.get_combatant(a.last('t'))
base = f'''embed -thumb {image} -color {color}'''
base += f''' -title "Testing" '''
base += f''' -desc "Testing stuff" '''

r = vroll("1d20 + 3")

damage = f"{r.total} [fire]"

base += f''' -f "test|{damage}" '''

base += f''' -f "{targ.name}|{targ.damage(damage).damage}" '''

base += f''' -footer "{targ.name}|{targ.hp_str()}" '''

return base
</drac2>